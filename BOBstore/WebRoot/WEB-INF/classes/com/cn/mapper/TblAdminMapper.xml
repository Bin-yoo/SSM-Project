<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.cn.mapper.TblAdminMapper">
	<select id="selectAllFun" resultType="tblAdmin">
		select * from tbl_street
	</select>
	
	<select id="selectByNameFun" parameterType="String" resultType="tblAdmin">
		select * from tbl_admin where adminName=#{name}
	</select>
</mapper>

